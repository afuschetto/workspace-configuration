{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#2e9599",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        },
        {
          "background": "#9a348e",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "session"
        },
        {
          "background": "#0080ff",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#c19c00",
          "foreground": "#193549",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "python"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}
